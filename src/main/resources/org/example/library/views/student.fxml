<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.library.controllers.StudentController" alignment="TOP_LEFT" spacing="10" padding="10">
    <Button fx:id="borrowBookButton" text="Borrow Book" onAction="#handleBorrowBook"/>
    <Button fx:id="returnBookButton" text="Return Book" onAction="#handleReturnBook"/>
    <Button fx:id="extendBookButton" text="Extend Book" onAction="#handleExtendBook"/>
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogout"/>
</VBox>
